<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.thoughtworks.funiesta.dao.UserMapper">

	<insert id="insertUser" parameterType="com.thoughtworks.funiesta.dao.User">
		INSERT INTO "Fun_User" (firstname,lastname,email,password) VALUES
		(#{firstname},#{lastname},#{email},#{password}) 
		</insert>

	<insert id="tryIt" parameterType="int">
		INSERT INTO "NEW"(id) VALUES(#{id});
	</insert>

</mapper>